{
  "name": "qrludo-generator",
  "productName": "QRLudo-Generator",
  "version": "1.1.0",
  "description": "Une application pour générer des Qr-Codes applicatifs utilisable sur l'application mobile QRLudo",
  "main": "index.js",
  "dependencies": {
    "electron": "^1.8.2",
    "electron-debug": "^1.5.0",
    "electron-packager": "^11.0.0",
    "google-auth-library": "^0.12.0",
    "googleapis": "^27.0.0",
    "jquery": "^3.3.1",
    "jquery-qrcode": "^1.0.0",
    "piexifjs": "^1.0.3",
    "popper.js": "^1.14.4",
    "zlib": "^1.0.5",
    "electron-settings":"^3.2.0",
    "request":"^2.88.0",
    "howler": "^2.1.1"
  },
  "scripts": {
    "install-linux": "electron-packager . qrludo-generator --overwrite --asar --platform=linux --out=release-builds",
